{
  "name": "@telefrek/couchbase",
  "displayName": "Telefrek Couchbase Library",
  "description": "This is a library that builds upon the our SQL package to support Couchbase via the couchbase project for issuing queries",
  "version": "1.0.0",
  "keywords": [
    "TypeScript",
    "SQL",
    "Couchbase"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./*.js": {
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "repository": {
    "url": "https://github.com/telefrek/sql",
    "type": "github"
  },
  "author": {
    "name": "Nathan Northcutt",
    "email": "nathan@telefrek.com"
  },
  "dependencies": {
    "@telefrek/type-utils": "workspace:^",
    "@telefrek/sql": "workspace:^",
    "couchbase": "^4.4.0"
  },
  "devDependencies": {
    "@types/couchbase": "^2.4.9"
  },
  "scripts": {
    "clean": "tsc --build --clean tsconfig.json",
    "format": "pnpx prettier --write .",
    "build": "tsc --build tsconfig.json",
    "lint": "pnpx eslint . --fix --ignore-pattern \"dist\""
  },
  "type": "module",
  "packageManager": "pnpm@9.3.0+sha256.e1f9e8d1a16607a46dd3c158b5f7a7dc7945501d1c6222d454d63d033d1d918f",
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  }
}
